<div class="header tree-header-content">
  {{> '/static/mustache/base_objects/tree_view_filter.mustache'}}

  <header class="header tree-header">
    <div class="row-fluid">
      <div class="span4">
        <div class="title-heading oneline">
          <span class="widget-col-title" data-field="title">
            Name
            <i class="fa fa-caret-up"></i>
          </span>
        </div>
      </div>
      <div class="span2">
        <div class="oneline">
          <span class="widget-col-title" data-field="email">
            Email
            <i class="fa fa-caret-up"></i>
          </span>
        </div>
      </div>
      <div class="span2">
        <div class="oneline">
          Authorizations
        </div>
      </div>
      <div class="span4">
        <ul class="tree-action-list">
          {{#if add_item_view}}
          <li>
            {{{renderLive add_item_view}}}
          </li>
          {{/if}}
        </ul>
      </div>
    </div>
  </header>
</div>
<div class="clearfix"></div>
